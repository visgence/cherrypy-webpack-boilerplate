{
    "name": "cherrypy-webpack-boilerplate",
    "version": "0.0.1",
    "description": "Boilerplate for cherrypy and webpack.",
    "author": "Visgence Inc.",
    "private": "true",
    "scripts": {
        "postinstall": "npm --prefix ./node_modules/node-sass/ run install && npm outdated",
        "start": "npm run build & ./teleceptorcmd runserver",
        "build": "npm run clean && npm run watch",
        "clean": "rm -fr static/dist/",
        "watch": "webpack --watch --progress --config ./webpack.development.config.js",
        "startProduction": "npm install && npm run buildProduction & python server.py",
        "buildProduction": "npm run clean && npm run webpackProduction",
        "webpackProduction": "webpack --progress --config ./webpack.production.config.js",
        "webpack": "webpack --progress --config ./webpack.development.config.js",
        "lint": "npm run htmllint & npm run scsslint & npm run eslint & npm run pythonlint",
        "pythonlint": "pep8 --exclude='*node_modules*' . --max-line-length=200",
        "eslint": "./node_modules/.bin/eslint src",
        "htmllint": "./node_modules/.bin/htmlhint --config .htmlhintrc src",
        "scsslint": "./node_modules/.bin/sass-lint src '**/*.scss' -v -q"
    },
    "dependencies": {
        "jquery": "3.2.1"
    },
    "devDependencies": {
        "babel-cli": "6.26.0",
        "babel-core": "6.26.0",
        "babel-eslint": "7.2.3",
        "babel-loader": "7.1.2",
        "babel-plugin-angularjs-annotate": "0.8.0",
        "babel-preset-env": "1.6.0",
        "babel-preset-es2015": "6.24.1",
        "css-loader": "0.28.7",
        "eslint": "4.6.1",
        "eslint-plugin-import": "2.7.0",
        "exports-loader": "0.6.4",
        "extract-text-webpack-plugin": "3.0.0",
        "file-loader": "0.11.2",
        "html-loader": "0.5.1",
        "htmlhint": "0.9.13",
        "imports-loader": "0.7.1",
        "less": "2.7.2",
        "node-sass": "4.5.3",
        "sass-lint": "1.11.1",
        "sass-loader": "6.0.6",
        "style-loader": "0.18.2",
        "uglifyjs-webpack-plugin": "0.4.6",
        "url-loader": "0.5.9",
        "webpack": "3.5.6",
        "webpack-bundle-analyzer": "2.9.0",
        "webpack-bundle-tracker": "0.2.0"
    }
}
